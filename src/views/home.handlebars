<div>
  {{#each products}}
    <div>
      <div>
        <img style="width: 50px; max-height: 50px" src={{thumbnail}}/>
        <div >
          <h2>{{title}}</h2>
          <p>{{description}}</p>
          <p>ID: {{id}}</p>
          <p>Código: {{code}}</p>
          <p>Categoría: {{category}}</p>
          <p>Stock: {{stock}}</p>
          <p>Precio: USD ${{price}}</p>
          <p>Status: {{status}}</p>
        </div>
      </div>
    </div>
  {{/each}}
</div>